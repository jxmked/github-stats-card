{
  "name": "github-stats-card",
  "version": "1.0.0",
  "description": "Create awesome Github stat card",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "tsc -p tsconfig.json -w",
    "build": "NODE_ENV=production rimraf dist && tsc -p tsconfig.json --sourceMap false --noEmitOnError true --listFiles true --diagnostics true",
    "prettier-format": "prettier --config .prettierrc \"./src\" \"./types\" --write",
    "serve": "NODE_ENV=development nodemon \"./dist/\" localhost 9000",
    "serve:offline": "NODE_ENV=development MODE=offline nodemon \"./dist/\" localhost 9000",
    "generate-sample-data": "node dist/scripts/index need-sample-data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxmked/github-stats-card.git"
  },
  "keywords": [
    "gh-stats-card",
    "card",
    "sci-fi",
    "statistic"
  ],
  "author": "Jovan De Guia <jovandeguia@gmail.com> (http://facebook.com/deguia25)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jxmked/github-stats-card/issues"
  },
  "homepage": "https://github.com/jxmked/github-stats-card#readme",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.1",
    "@types/numeral": "^2.0.2",
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "mkdirp": "^2.1.5",
    "nodemon": "^2.0.21",
    "numeral": "^2.0.6",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "svgo": "^3.0.2",
    "svgson": "^5.2.1",
    "typescript": "^4.9.5"
  }
}
